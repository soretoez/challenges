#include <iostream>
#include <vector>

using namespace std;

int main(){

    vector <int> vector_1 {};
    
    cout <<"\nType 10 to add to vector 1" <<endl;
    
    int vector_to_add1 {0};
    cin >> vector_to_add1;
    vector_1.push_back(vector_to_add1);
    
    cout <<"\nType 20 to add to vector 1" <<endl;
    cin >> vector_to_add1;
    vector_1.push_back(vector_to_add1);
    
    cout <<vector_1.at(0) <<endl;
    cout <<vector_1.at(1) <<endl;
    cout <<"\nThere are " << vector_1.size() << " numbers in vector 1" <<endl;
    
    vector <int> vector_2 {};
    
    cout <<"\ntype 100 to add to vector 2:" <<endl;
    
    int vector_to_add2 {0};
    cin >> vector_to_add2;
    vector_2.push_back(vector_to_add2);
    
    cout <<"\nType 200 to add to vector 2" <<endl;
    cin >> vector_to_add2;
    vector_2.push_back(vector_to_add2);
    
    cout << vector_2.at(0) <<endl;
    cout << vector_2.at(1) <<endl;
    cout <<"\nThere are " <<vector_2.size() << " numbers in vector 2" <<endl;
    
    cout << "\n 2D vector" <<endl;
    vector <vector<int>> vector_2d {};
    vector_2d.push_back(vector_1);
    vector_2d.push_back(vector_2);
    
    cout << vector_2d[0][0] <<" "<< vector_2d[0][1] <<endl;
    cout << vector_2d[1][0] <<" "<< vector_2d[1][1] <<endl;
*/ line 46 was where I went wrong I did [0][2] and [0][3]
   
